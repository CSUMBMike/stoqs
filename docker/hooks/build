#!/bin/bash
# The working directory in docker cloud is $STOQS_HOME/docker
STOQS_HOME=$(dirname `pwd`)
echo "STOQS_HOME_UI set to $STOQS_HOME_UI"
echo "STOQS_HOME set to $STOQS_HOME"
echo "STOQS_HOME listing"
ls -l $STOQS_HOME
echo "env:"
env
docker-compose -f docker-compose.test.yml build -e STOQS_HOME=$STOQS_HOME
