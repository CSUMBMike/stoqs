#!/bin/bash
# The working directory in docker cloud is $STOQS_HOME/docker
STOQS_HOME=$(dirname `pwd`)
echo "STOQS_HOME set to $STOQS_HOME"
echo "STOQS_HOME listing"
ls -l $STOQS_HOME
echo "Adding this setting to $STOQS_HOME/docker/test.env..."
cat <<EOT >> $STOQS_HOME/docker/test.env
STOQS_HOME=$STOQS_HOME

EOT
docker-compose -f docker-compose.test.yml build
